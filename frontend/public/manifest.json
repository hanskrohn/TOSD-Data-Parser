{
  "name": "ToSBot Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["background.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "manifest_version": 3,
  "permissions": ["scripting", "tabs", "notifications"],
  "host_permissions": ["*://*/*"]
}
