{
  "name": "ToSBot Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage", "active tab", "declarativeContent"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open ToSBot"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
