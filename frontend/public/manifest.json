{
  "name": "ToSBot Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["background.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "see suggestions for this site"
  }
}
